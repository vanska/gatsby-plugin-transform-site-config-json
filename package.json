{
  "name": "gatsby-plugin-transform-site-config-json",
  "description": "Transforms site config JSON to Gatsby data node",
  "version": "1.0.1",
  "author": "Mikko Vänskä <mikkovanska.com@gmail.com>",
  "dependencies": {
    "@babel/runtime": "^7.10.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "babel-preset-gatsby-package": "^0.5.2",
    "cross-env": "^5.2.1"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "i18n"
  ],
  "license": "MIT",
  "peerDependencies": {
    "gatsby": ">2.0.0-alpha"
  },
  "repository": "https://github.com/vanska/gatsby-plugin-transform-site-config-json",
  "scripts": {
    "build": "babel src --out-dir . --ignore \"**/__tests__\"",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore \"**/__tests__\""
  },
  "engines": {
    "node": ">=10.13.0"
  }
